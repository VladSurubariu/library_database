@model project.Models.MemberModel

@{
    ViewData["Title"] = "MemberDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>MemberDetails</h1>

<div>
    <h4>MemberModel</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-3">
            Member Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MemberName)
        </dd>
        <dt class = "col-sm-3">
            Member Adress
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MemberAdress)
        </dd>
        <dt class = "col-sm-3">
            Member Phone Number
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MemberPhoneNumber)
        </dd>
        <dt class = "col-sm-3">
            Member Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MemberEmail)
        </dd>
        <dt class = "col-sm-3">
            Membership Expiration Date
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MemberMembershipExpirationDate)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { id = Model.MemberID }) |
    <a asp-action="Index">Back to List</a>
</div>
